{
  "name": "knt2-vbot",
  "version": "1.0.0",
  "description": "Kanata 2nd Voice Bot is a smart speaker over the Internet, supports lots of languages.",
  "author": "Kanata",
  "license": "MIT",
  "private": true,
  "scripts": {
    "init": "npm i && node init",
    "prebuild": "rimraf ./dist && cpx ./src/assets/**/* ./dist && cpx ./src/plugins/!(!*)/assets/**/* ./dist/plugins",
    "build": "npm run prebuild && webpack --mode production",
    "start": "http-server ./dist/ -a localhost -c-1 -o",
    "dev": "npm run prebuild && webpack-dev-server",
    "lint": "eslint --ext js,vue ./src/app/ ./src/plugins/ ./plugin-example/",
    "beatify": "eslint --fix --ext js,vue ./src/app/ ./src/plugins/ ./plugin-example/",
    "firebase": "firebase"
  },
  "dependencies": {
    "@mdi/font": "^4.9.95",
    "axios": "^0.19.2",
    "axios-jsonp": "^1.0.4",
    "cpx": "^1.5.0",
    "css-loader": "^3.4.2",
    "file-loader": "^5.1.0",
    "he": "^1.2.0",
    "howler": "^2.1.3",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.12.1",
    "js-queue": "^2.0.0",
    "js-yaml": "^3.13.1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.28",
    "rimraf": "^3.0.2",
    "vue": "^2.6.11",
    "vue-loader": "^15.9.0",
    "vue-template-compiler": "^2.6.11",
    "vuetify": "^2.2.15",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "ws": "^7.2.1"
  },
  "devDependencies": {
    "eslint": "^7.0.0-alpha.1",
    "eslint-plugin-vue": "^6.2.1",
    "firebase-tools": "^7.14.0"
  }
}
